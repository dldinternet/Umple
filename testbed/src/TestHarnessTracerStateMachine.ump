class LightFixtureA
{
  
  status {
    Open {
      flip -> Closed;
    }
    Closed {
      flip -> Open;
      do 
      {  
        Thread.sleep(400);
        measureClose();
      }
    }
    // trace state with do activity
    trace Closed;
  }
  
  void measureClose() {};
}


class LightFixtureB
{ 
  bulb 
  {
