generate Papyrus;
namespace example;

class Garage{
 GarageDoor {
      Closed {
        exit/ {triggerNormalEnergyMode();}
        pressButton -> Opening;
      }
     Opening {
       entry/{runMotorForward();}
       openingCompleted -> Open;
     }
     Open {
       pressButton  -> Closing;
     }
     Closing {
       entry/{runMotorInReverse();}
       closingCompleted -> Closed;
     }
   }
}

